journal = {Artificial Life},
keywords = {crowdfunding,crowdsourcing,html5,open science,public outreach,web technologies,webal},
pages = {364--407},
title = {{WebAL Comes of Age: A Review of the First 21 Years of Artificial Life on the Web}},
