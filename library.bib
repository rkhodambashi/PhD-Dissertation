title = {{Remotely Triggered Locomotion of Hydrogel Mag-bots in Confined Spaces}},
url = {http://dx.doi.org/10.1038/s41598-017-16265-w},
volume = {7},
year = {2017}
}
@article{Li2005a,
abstract = {Ultra-fine fibrous membranes were prepd. by electrospinning of aq. mixts. of poly(acrylic acid) and poly(vinyl alc.) at 17{\%}-83{\%} PAA or 0.14-3.5 COOH/OH molar ratios and crosslinked by heat-induced (140 Â°C, 5 min) esterification. The fiber diams. increased from 270 to 450 nm with increasing PAA contents. The swelling ratio of the fibrous hydrogels increased up to 31 times their dry wt. with increasing pH from 2 to 7, and most significantly between pH 4 and pH 5. The liq. uptake of the fibrous hydrogels was attributed to both the liq. diffused into the fibers and that held in the inter-fiber pores. About 53{\%}, 35{\%}, 43{\%} and 37{\%} of the swelling was contributed by the liq. in the inter-fiber pores when the fibrous hydrogels were swollen at pH 2, 4, 5 and 7, resp. The fully swollen fibrous membranes could be triggered by an applied elec. field to swell further. Such addnl. swelling was dependent upon the polymer compns., strength of elec. field and pH, and was reversible. [on SciFinder(R)]},
