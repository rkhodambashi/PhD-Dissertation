title = {{Novel Design of a Soft Lightweight Pneumatic Continuum Robot Arm with Decoupled Variable Stiffness and Positioning}},
url = {http://online.liebertpub.com/doi/10.1089/soro.2016.0066},
volume = {5},
year = {2017}
}
