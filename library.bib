journal = {Journal of Bionic Engineering},
keywords = {Biomimetics,Locomotion generation,Mathematical model,Motion library,Robotic fish},
number = {3},
pages = {251--264},
publisher = {Jilin University},
title = {{Locomotion generation and motion library design for an anguilliform robotic fish}},
url = {http://dx.doi.org/10.1016/S1672-6529(13)60221-8},
volume = {10},
year = {2013}
}
