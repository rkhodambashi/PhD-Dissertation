doi = {10.1016/j.snb.2016.03.060},
issn = {09254005},
journal = {Sensors and Actuators, B: Chemical},
keywords = {Actuating pressure,Hydrogel,Microelectromechanical systems,Response time,Thermal actuator,Thermoelectrical cooler},
pages = {900--908},
publisher = {Elsevier B.V.},
