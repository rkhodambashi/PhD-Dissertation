file = {:G$\backslash$:/My Drive/ASU/Research/Mendeley/Drost et al/Journal of Biomechanics/Drost et al. - 2003 - Spatial and temporal heterogeneity of superficial muscle strain during in situ fixed-end contractions.pdf:pdf},
issn = {00219290},
journal = {Journal of Biomechanics},
keywords = {Isometric contraction,Mechanical strain,Motor endplate,Skeletal muscle,Superficial deformation},
month = {jul},
number = {7},
pages = {1055--1063},
pmid = {12757815},
title = {{Spatial and temporal heterogeneity of superficial muscle strain during in situ fixed-end contractions}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S002192900200461X},
volume = {36},
year = {2003}
