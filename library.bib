pages = {107544},
publisher = {The Authors.},
title = {{4D printing with robust thermoplastic polyurethane hydrogel-elastomer trilayers}},
url = {https://doi.org/10.1016/j.matdes.2018.107544},
volume = {163},
year = {2019}
