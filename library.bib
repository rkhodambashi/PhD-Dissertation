journal = {IEEE Conference on Technologies for Practical Robot Applications, TePRA},
keywords = {Dynamic characterization,Fluidic elastomer actuator,Optimal design,Soft robotic snake},
pages = {1--6},
title = {{Design improvements and dynamic characterization on fluidic elastomer actuators for a soft robotic snake}},
year = {2014}
}
@article{Hiller2012a,
abstract = {This paper describes a 2D and 3D simulation engine that quantitatively models the statics, dynamics, and non-linear deformation of heterogeneous soft bodies in a computationally efficient manner. There is a large body of work simulating compliant mechanisms. These normally assume small deformations with homogeneous material properties actuated with external forces. There is also a large body of research on physically-based deformable objects for applications in computer graphics with the purpose of generating realistic appearances at the expense of accuracy. Here we present a simulation framework in which an object may be composed of any number of interspersed materials with varying properties (stiffness, density, etc.) to enable true heterogeneous multi-material simulation. Collisions are handled to prevent self-penetration due to large deformation, which also allows multiple bodies to interact. A volumetric actuation method is implemented to impart motion to the structures which opens the door to the design of novel structures and mechanisms. The simulator was implemented efficiently such that objects with thousands of degrees of freedom can be simulated at suitable framerates for user interaction using a single thread of a typical desktop computer. The code is written in platform agnostic C++ and is fully open source. This research opens the door to the dynamic simulation of freeform 3D multi-material mechanisms and objects in a manner suitable for design automation.},
archivePrefix = {arXiv},
arxivId = {1212.2845},
author = {Hiller, Jonathan and Lipson, Hod},
eprint = {1212.2845},
keywords = {heterogeneous,non-linear mechanism,soft body},
title = {{Dynamic Simulation of Soft Heterogeneous Objects}},
url = {http://arxiv.org/abs/1212.2845},
year = {2012}
}
@article{Volkov2013,
abstract = {Biomechanics of morphing structures in the Venus flytrap has attracted the attention of scientists during the last 140 years. The trap closes in a tenth of a second if a prey touches a trigger hair twice. The driving force of the closing process is most likely due to the elastic curvature energy stored and locked in the leaves, which is caused by a pressure differential between the upper and lower layers of the leaf. The trap strikes, holds and compresses the prey. We have developed new methods for measuring all these forces involved in the hunting cycle. We made precise calibration of the piezoelectric sensor and performed direct measurements of the average impact force of the trap closing using a high speed video camera for the determination of time constants. The new equation for the average impact force was derived. The impact average force between rims of two lobes in the Venus flytrap was found equal to 149. mN and the corresponding pressure between the rims was about 41. kPa. Direct measurements of the constriction force in the trap of Dionaea muscipula was performed during gelatin digestion. This force increases in the process of digestion from zero to 450. mN with maximal constriction pressure created by the lobes reaching to 9. kPa. The insects and different small prey have little chance to escape after the snap of the trap. The prey would need to overpower the "escaping" force which is very strong and can reach up to 4. N. {\textcopyright} 2012 Elsevier GmbH.},
author = {Volkov, Alexander G. and Harris, Shawn L. and Vilfranc, Chrystelle L. and Murphy, Veronica A. and Wooten, Joseph D. and Paulicin, Henoc and Volkova, Maia I. and Markin, Vladislav S.},
doi = {10.1016/j.jplph.2012.08.009},
issn = {01761617},
journal = {Journal of Plant Physiology},
keywords = {Carnivorous plant,Electrophysiology,Morphing structures,Plant biomechanics,Venus flytrap},
number = {1},
pages = {25--32},
title = {{Venus flytrap biomechanics: Forces in the Dionaea muscipula trap}},
volume = {170},
year = {2013}
}
@article{Yang2018,
abstract = {Hybrid ionicallyâ€“covalently crosslinked doubleâ€network {\{}(DN){\}} hydrogels are attracting increasing attention on account of their selfâ€recovery ability and fatigue resistance, but their relative low mechanical strength and tedious performance adjustment severely limit their applications. Herein, a new strategy to concurrently fabricate hybrid ionicâ€“covalent {\{}DN{\}} hydrogels and modulate their structures and mechanics is reported, in which an in situ formed chitosan ionic network is incorporated by postâ€crosslinking the chitosanâ€based composite hydrogel using multivalent anions solutions. The obtained hybrid {\{}DN{\}} hydrogels exhibit predominant mechanical properties including superior elastic modulus, high tensile strength, and ultrahigh fracture energy because of the more efficient energy dissipation of rigid shortâ€chain chitosan network. Notably, the swollen hydrogels still remain mechanically strong and tough even after immersion in water for 24 h. More significantly, simply changing the postâ€crosslinking time can vary the compactness and rigidity of the chitosan network in situ, achieving flexible and efficient modulation of the structures and mechanics of the hybrid {\{}DN{\}} hydrogels. This study opens up a new horizon in the preparation and regulation of {\{}DN{\}} hydrogels for promising applications in tissue scaffolds, actuators, and wearable devices. Hybrid doubleâ€network hydrogels are fabricated by a simple strategy and their structures and mechanics are modulated by incorporating an in situ formed chitosan ionic network via postâ€crosslinking chitosanâ€based composite hydrogels using multivalent anions solutions. This study opens up a new horizon in the preparation and regulation of stiff and tough hydrogels in loadâ€bearing applications.},
author = {Yang, Yanyu and Wang, Xing and Yang, Fei and Wang, Luning and Wu, Decheng},
doi = {10.1002/adma.201707071},
issn = {15214095},
journal = {Advanced Materials},
keywords = {DN hydrogels,chitosan,energy dissipation,ionic coordination,tunable mechanics},
number = {18},
pages = {1--9},
pmid = {29577453},
title = {{Highly Elastic and Ultratough Hybrid Ionicâ€“Covalent Hydrogels with Tunable Structures and Mechanics}},
volume = {30},
year = {2018}
}
@article{Guo2013,
abstract = {Combination of soft active hydrogels with hard passive polymers gives rise to all-polymer{\$}\backslash{\$}n composites. The hydrogel is sensitive to external stimuli while the passive polymer is inert.{\$}\backslash{\$}n Utilizing the different behaviors of two materials subject to environmental variation, for example{\$}\backslash{\$}n temperature, results in self-folding soft machines. We report our efforts to model the programmable{\$}\backslash{\$}n deformation of self-folding structures with temperature-sensitive hydrogels. The self-folding{\$}\backslash{\$}n structures are realized either by constructing a bilayer structure or by incorporating hydrogels as{\$}\backslash{\$}n hinges. The methodology and the results may aid the design, control and fabrication of 3D complex{\$}\backslash{\$}n structures from 2D simple configurations through self-assembly.},
