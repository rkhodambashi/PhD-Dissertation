title = {{An under actuated robotic arm with adjustable stiffness shape memory polymer joints}},
volume = {2015-June},
year = {2015}
}
@article{Tolley2014a,
abstract = {Self-folding is an approach used frequently in nature for the efficient fabrication of structures,$\backslash$r but is seldom used in engineered systems. Here, self-folding origami are presented, which consist of$\backslash$r shape memory composites that are activated with uniform heating in an oven. These composites are$\backslash$r rapidly fabricated using inexpensive materials and tools. The folding mechanism based on the$\backslash$r in-plane contraction of a sheet of shape memory polymer is modeled, and parameters for the design of$\backslash$r composites that self-fold into target shapes are characterized. Four self-folding shapes are$\backslash$r demonstrated: a cube, an icosahedron, a flower, and a Miura pattern; each of which is activated in$\backslash$r an oven in less than 4 min. Self-sealing is also investigated using hot melt adhesive, and the$\backslash$r resulting structures are found to bear up to twice the load of unsealed structures.},
