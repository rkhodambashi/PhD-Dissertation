keywords = {Collision Detection,Haptic Feedback,Hexahedral Mesh,Smooth Particle Hydrodynamic},
pages = {283--321},
title = {{SOFA: A Multi-Model Framework for Interactive Physical Simulation}},
url = {http://link.springer.com/10.1007/8415{\_}2012{\_}125},
