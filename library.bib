volume = {6},
year = {2015}
}
@article{Pagitz2013a,
abstract = {Plants such as Dionaea muscipula (Venus Flytrap) can change the shape of their shell-like leaves by actively altering the cell pressures. These leaves are hydraulic actuators that do not require any complex controls and that possess an energy efficiency that is unmatched by natural or artificial muscles (Huber et al 1997 Proc. R. Soc. A 453 2185-205). We extend our previous work (Pagitz et al 2012 Bioinspir. Biomim. 7 016007) on pressure-actuated cellular structures by introducing a concept for shape-changing shell-like structures that can significantly alter their Gaussian curvature. The potential of this concept is demonstrated by a hemispherical shell that can reversibly change the sign of its Gaussian curvature. Furthermore, it is shown that a snap-through behaviour, similar to the one known from Dionaea muscipula, can be achieved by lowering the pressure in a single layer of cells. {\textcopyright} 2013 IOP Publishing Ltd.},
author = {Pagitz, M. and Bold, J.},
doi = {10.1088/1748-3182/8/1/016010},
issn = {17483182},
journal = {Bioinspiration and Biomimetics},
number = {1},
pages = {0--11},
title = {{Shape-changing shell-like structures}},
volume = {8},
year = {2013}
}
@article{Wang2013a,
abstract = {We describe a facile fabrication of solid-state actuators by coating common polymer films with a layer of carbon nanotubes. The composite material actuators are multifunctional energy transducers and were powered by heat, light, or electricity. The maximum observed force produced by an actuator was 60 times its own weight. Actuators were also demonstrated to bend more than 90Â°. The actuators were repeatedly activated for nearly 50,000 cycles without significant loss of performance for a sub-hertz actuator and 1,000,000 cycles in the case of a 30 Hz actuator. The utility of these devices was demonstrated by creating a walking robot.},
author = {Wang, Chengwei and Nosaka, Takayuki and Yost, Brett and Zimmerman, Benjamin and Sutton, Emily D and Kincaid, Eric and Keberle, Katelyn and Iqbal, Qazi Aied and Mendez, Rodrigo and Markowitz, Sivan and Liu, Pai and Alford, Terry L and Chan, Candace K and Chan, Kwai S and O'Connell, Michael J},
