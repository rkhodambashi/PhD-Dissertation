issn = {2169-5172},
journal = {Soft Robotics},
keywords = {bending module,elastomers,origami shell,reconfigurability,soft pneumatic actuators,soft robots},
number = {3},
pages = {109--119},
title = {{Design and Analysis of a Soft Pneumatic Actuator with Origami Shell Reinforcement}},
