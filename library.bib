@article{Santaniello2015,
abstract = {Wireless Sensor Networks (WSNs) have a very large growth in research$\backslash$nin recent years. They are essentially ad-hoc networks, capable of$\backslash$nprocessing, sensing and transmitting wireless information. These$\backslash$nare networks where the specificity of applications becomes something$\backslash$ncommon, due to various types of restrictions (e.g., processing, bandwidth,$\backslash$netc), and energy consumption is the most important restriction. Trying$\backslash$nto solve this problem, we present a new approach to saving energy$\backslash$nin WSN, based on automatic sensors reconfiguration. Our approach$\backslash$nconsists of three components: a middleware to handle the heterogeneity$\backslash$nof data and applications, an ontology to give a formal semantics$\backslash$nto data and a reconfiguration module to communicate with ontology$\backslash$nand middleware, and process queries. Some assessments were made in$\backslash$norder to show its feasibility, using a case study based on energy$\backslash$nconsumption. Finally, we argue that its use is feasible and attractive.},
