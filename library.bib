}
@article{Shen2017,
abstract = {In this study, soft hydrogel crawlers with remote magnetic-responsive
motility in confined spaces have been developed. Inspired by the motion
of maggots, the hydrogel crawlers can reversibly contract and elongate
their body controlled by repeatedly switching on/off an alternating
magnetic field. Based on the cyclic deformation, the hydrogel crawlers
can move peristaltically in a confined space that is coated with
asymmetric micro-patterns. The dependence of the hydrogel motility on
