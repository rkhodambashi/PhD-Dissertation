issn = {0169-1864},
journal = {Advanced Robotics},
keywords = {Octopus,muscular hydrostats,shape memory alloy,soft robot},
month = {jan},
number = {7},
pages = {709--727},
title = {{Soft Robot Arm Inspired by the Octopus}},
url = {https://www.tandfonline.com/doi/full/10.1163/156855312X626343},
volume = {26},
year = {2012}
