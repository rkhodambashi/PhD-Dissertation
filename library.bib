journal = {Journal of Polymer Research},
keywords = {Hydrogels,Morphology,Polyacrylamide,Pore size,Solvent effect,Swelling,Water/solvent},
number = {1},
publisher = {Journal of Polymer Research},
title = {{Effect of the synthesis solvent in swelling ability of polyacrylamide hydrogels}},
volume = {27},
year = {2020}
