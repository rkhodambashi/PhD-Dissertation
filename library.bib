@article{Watanabe2019,
abstract = {This paper presents fabrication and actuation methods for a soft microrobot with a hybrid structure composed of soft microactuators and a rigid supporting body. This hybrid structure enables actuation of the microrobot with independent driving of multiple actuators to provide complex movement like that of living microorganisms. We use the temperature-responsive hydrogel poly(N-isopropylacrylamide) (PNIPAAm) as a soft microactuator. PNIPAAm swells with water at low temperature but shrinks at high temperature. This volume change thus allows PNIPAAm to be used as an actuator by controlling its temperature. We successfully fabricated the microrobot with its soft-rigid hybrid structure composed of PNIPAAm and rigid photoresist using a multilayered microfabrication process. In addition, we used a sacrificial layer process to release the fabricated microrobot from the substrate to allow it to move freely. To actuate the microrobot, we mixed PNIPAAm with graphene, which has a high photothermal conversion efficiency. The temperature of the soft actuator when mixed with graphene can be increased by irradiating it with light. Therefore, actuation of the microrobot is achieved by sequentially irradiating the microactuators with focused light. We present the fabrication, release and partial actuation of the microrobot to demonstrate the feasibility of the proposed microrobot with the soft-rigid hybrid structure in this paper.},
author = {Watanabe, Tomoki and Yokoyama, Yoshiyuki and Hayakawa, Takeshi},
doi = {10.1186/s40648-019-0140-3},
issn = {21974225},
journal = {ROBOMECH Journal},
keywords = {Gel actuator,Graphene,Microrobot,PNIPAAm,Soft robot},
number = {1},
publisher = {Springer International Publishing},
title = {{Structurally isolated photoactuation of graphene-mixed temperature-responsive hydrogels in soft-rigid series structure}},
url = {https://doi.org/10.1186/s40648-019-0140-3},
volume = {6},
year = {2019}
}
@article{Peraza-Hernandez2014a,
abstract = {Origami, the ancient art of paper folding, has inspired the design of engineering devices and$\backslash$r structures for decades. The underlying principles of origami are very general, which has led to$\backslash$r applications ranging from cardboard containers to deployable space structures. More recently,$\backslash$r researchers have become interested in the use of active materials (i.e., those that convert various$\backslash$r forms of energy into mechanical work) to effect the desired folding behavior. When used in a$\backslash$r suitable geometry, active materials allow engineers to create self-folding structures. Such$\backslash$r structures are capable of performing folding and/or unfolding operations without being kinematically$\backslash$r manipulated by external forces or moments. This is advantageous for many applications including$\backslash$r space systems, underwater robotics, small scale devices, and self-assembling systems. This article$\backslash$r is a survey and analysis of prior work on active self-folding structures as well as methods and$\backslash$r tools available for the design of folding structures in general and self-folding structures in$\backslash$r particular. The goal is to provide researchers and practitioners with a systematic view of the$\backslash$r state-of-the-art in this important and evolving area. Unifying structural principles for active$\backslash$r self-folding structures are identified and used as a basis for a quantitative and qualitative$\backslash$r comparison of numerous classes of active materials. Design considerations specific to folded$\backslash$r structures are examined, including the issues of crease pattern identification and fold kinematics.$\backslash$r Although few tools have been created with active materials in mind, many of them are useful in the$\backslash$r overall design process for active self-folding structures. Finally, the article concludes with a$\backslash$r discussion of open questions for the field of origami-inspired engineering.},
author = {Peraza-Hernandez, Edwin A. and Hartl, Darren J. and Malak, Richard J. and Lagoudas, Dimitris C.},
doi = {10.1088/0964-1726/23/9/094001},
file = {::},
isbn = {0964-1726},
issn = {1361665X},
journal = {Smart Materials and Structures},
keywords = {active materials,design,morphing structures,origami,origami engineering,review,smart structures},
number = {9},
publisher = {IOP Publishing},
title = {{Origami-inspired active structures: A synthesis and review}},
volume = {23},
year = {2014}
}
