journal = {IEEE International Conference on Intelligent Robots and Systems},
pages = {6199--6206},
title = {{Gait Learning for Soft Microrobots Controlled by Light Fields}},
year = {2018}
}
@article{Belding2018,
abstract = {{\textcopyright} 2018 WILEY-VCH Verlag GmbH {\&} Co. KGaA, Weinheim. This article describes a new principle for designing soft or 'semisoft' pneumatic actuators: SLiT (for SLit-in-Tube) actuators. Inflating an elastomeric balloon, when enclosed by an external shell (a material with higher Young's modulus) containing slits of different directions and lengths, produces a variety of motions, including bending, twisting, contraction, and elongation. The requisite pressure for actuation depends on the length of the slits, and this dependence allows sequential actuation by controlling the applied pressure. Different actuators can also be controlled using external "sliders" that act as reprogrammable "on-off" switches. A pneumatic arm and a walker constructed from SLiT actuators demonstrate their ease of fabrication and the range of motions they can achieve.},
