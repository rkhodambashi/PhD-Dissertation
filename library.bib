journal = {Smart Materials and Structures},
keywords = {4D printing,origami,shape memory polymers},
number = {9},
title = {{Active origami by 4D printing}},
volume = {23},
year = {2014}
}
@article{Liu2018c,
abstract = {3D printing has been intensively explored to fabricate customized structures of responsive materials including hydrogels, liquid-crystal elastomers, shape-memory polymers, and aqueous droplets. Herein, a new method and material system capable of 3D-printing hydrogel inks with programed bacterial cells as responsive components into large-scale (3 cm), high-resolution (30 $\mu$m) living materials, where the cells can communicate and process signals in a programmable manner, are reported. The design of 3D-printed living materials is guided by quantitative models that account for the responses of programed cells in printed microstructures of hydrogels. Novel living devices are further demonstrated, enabled by 3D printing of programed cells, including logic gates, spatiotemporally responsive patterning, and wearable devices.},
author = {Liu, Xinyue and Yuk, Hyunwoo and Lin, Shaoting and Parada, German Alberto and Tang, Tzu Chieh and Tham, El{\'{e}}onore and de la Fuente-Nunez, Cesar and Lu, Timothy K. and Zhao, Xuanhe},
