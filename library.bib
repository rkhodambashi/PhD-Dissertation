@article{Preston2019,
abstract = {Although soft devices (grippers, actuators, and elementary robots) are rapidly becoming an integral part of the broad field of robotics, autonomy for completely soft devices has only begun to be developed. Adaptation of conventional systems of control to soft devices requires hard valves and electronic controls. This paper describes completely soft pneumatic digital logic gates having a physical scale appropriate for use with current (macroscopic) soft actuators. Each digital logic gate utilizes a single bistable valveâ€”the pneumatic equivalent of a Schmitt triggerâ€”which relies on the snap-through instability of a hemispherical membrane to kink internal tubes and operates with binary high/low input and output pressures. Soft, pneumatic NOT, AND, and OR digital logic gatesâ€”which generate known pneumatic outputs as a function of one, or multiple, pneumatic inputsâ€”allow fabrication of digital logic circuits for a setâ€“reset latch, two-bit shift register, leading-edge detector, digital-to-analog converter (DAC), and toggle switch. The DAC and toggle switch, in turn, can control and power a soft actuator (demonstrated using a pneu-net gripper). These macroscale soft digital logic gates are scalable to high volumes of airflow, do not consume power at steady state, and can be reconfigured to achieve multiple functionalities from a single design (including configurations that receive inputs from the environment and from human users). This work represents a step toward a strategy to develop autonomous controlâ€”one not involving an electronic interface or hard componentsâ€”for soft devices.},
author = {Preston, Daniel J. and Rothemund, Philipp and Jiang, Haihui Joy and Nemitz, Markus P. and Rawson, Jeff and Suo, Zhigang and Whitesides, George M.},
doi = {10.1073/pnas.1820672116},
file = {:G$\backslash$:/My Drive/ASU/Research/Mendeley/Preston et al/Proceedings of the National Academy of Sciences/Preston et al. - 2019 - Digital logic for soft devices.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Artificial intelligence,Buckling,Control,Humanâ€“soft device interaction,Logic},
month = {apr},
number = {16},
pages = {7750--7759},
pmid = {30923120},
title = {{Digital logic for soft devices}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1820672116},
volume = {116},
year = {2019}
}
